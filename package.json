		"@sveltejs/kit": "2.21.1",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@tailwindcss/vite": "4.1.8",
		"@types/dom-view-transitions": "1.0.6",
		"@types/eslint": "9.6.1",
		"@types/google.maps": "3.58.1",
		"@types/node": "22.15.29",
		"@types/ws": "8.18.1",
		"eslint": "9.28.0",
		"eslint-config-prettier": "10.1.5",
