{
	"name": "tvupweb",
	"version": "3.47.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "TZ=America/Toronto vite dev --host",
		"build": "npm run lint && vite build",
		"preview": "npm run build && vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
