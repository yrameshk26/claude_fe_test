		"tailwind-merge": "3.3.0",
		"tailwindcss": "4.1.8",
		"typescript": "5.8.3",
		"typescript-eslint": "8.33.0",
		"vite": "6.3.5"
	},
	"dependencies": {
		"@sentry/sveltekit": "9.24.0",
		"class-variance-authority": "^0.7.1",
